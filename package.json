{
    "name": "it4090-api",
    "version": "1.0.0",
    "engines": {
        "node": ">=20.0.0"
    },
    "description": "clothing store api",
    "main": "src/app.js",
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "dev": "nodemon src/app.js",
        "start": "node src/app.js",
        "migrate:up": "npx sequelize-cli db:migrate",
        "migrate:undo": "npx sequelize-cli db:migrate:undo",
        "lint": "eslint \"src/**/*.js\"",
        "format": "prettier --write \"src/**/*.+(js|jsx|json|yml|yaml|css|md|vue)\"",
        "prepare": "husky install"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/nghienthamnien/IT4090-api.git"
    },
    "author": "Binhpt",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/nghienthamnien/IT4090-api/issues"
    },
    "homepage": "https://github.com/nghienthamnien/IT4090-api#readme",
    "dependencies": {
        "bcrypt": "^5.1.0",
        "callsites": "^4.0.0",
        "cookie-parser": "^1.4.6",
        "cors": "^2.8.5",
        "date-fns": "^2.30.0",
        "dotenv": "^16.0.3",
        "express": "^4.18.2",
        "joi": "^17.9.2",
        "js-yaml": "^4.1.0",
        "jsonwebtoken": "^9.0.0",
        "lodash": "^4.17.21",
        "morgan": "^1.10.0",
        "multer": "^1.4.5-lts.1",
